page {
  background-color: #ECEEEB;
}
.question_main .main_content {
  display: flex;
  margin-top: 20rpx;
}
.question_main .main_content .content_icon {
  flex: 1;
}
.question_main .main_content .content {
  flex: 9;
  background-color: white;
  border-radius: 10rpx;
  margin-right: 10rpx;
  padding: 10rpx;
  font-size: 34;
}
.placeholder-view {
  width: 100%;
  height: 150rpx;
}
.question_main .main_input {
  display: flex;
  height: 90rpx;
  width: 100%;
  padding: 10rpx;
}
.question_main .main_input .inpunt_item {
  display: flex;
}
.question_main .main_input .inpunt_item .input_content {
  background-color: #ECEEEB;
  border-radius: 10px;
  flex: 8 ;
  height: 100%;
}
.question_main .main_input .inpunt_item .input_button {
  margin-left: 20rpx;
  border-radius: 10px;
  background-color: #55b7f8;
  flex: 1;
  height: 100%;
  text-align: center;
  align-items: center;
  font-size: 26rpx;
  color: #ECEEEB;
}
.footer {
  width: 100%;
  background-color: white;
  position: fixed;
  bottom: 0px;
}
.footer .predict_main .predict_content {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  border: 3rpx solid #b1afaf;
  border-radius: 20rpx;
  background-color: #ecebeb;
  padding: 10rpx;
}
